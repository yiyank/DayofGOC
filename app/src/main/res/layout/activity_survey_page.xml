<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SurveyPage">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:text="SURVEY FEEDBACK"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="79dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="344dp"
        android:layout_height="wrap_content"
        android:text="
        The GOC Team hopes that you’re enjoying your experience! We strive each year to continue improving the conference and would appreciate your feedback.  "
        android:textSize="22sp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="123dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="
        Please let us know if there’s anything you’d like to see more of, less of or even totally remove!"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="272dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="202dp"
        android:layout_height="45dp"
        android:background="#ffcb05"
        android:text="Link to Survey"
        android:textColor="#ffffff"
        android:textSize="22dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="414dp" />

</android.support.constraint.ConstraintLayout>